:set ($convert->("\D7")) ("%E0%B8%B7")
:set ($convert->("\D8")) ("%E0%B8%B8")
:set ($convert->("\D9")) ("%E0%B8%B9")
:set ($convert->("\DA")) ("%E0%B8%BA")
:set ($convert->("\DB")) ("%E0%B8%BB")
:set ($convert->("\DC")) ("%E0%B8%BC")
:set ($convert->("\DD")) ("%E0%B8%BD")
:set ($convert->("\DE")) ("%E0%B8%BE")
:set ($convert->("\DF")) ("%E0%B8%BF")
:set ($convert->("\E0")) ("%E0%B9%80")
:set ($convert->("\E1")) ("%E0%B9%81")
:set ($convert->("\E2")) ("%E0%B9%82")
:set ($convert->("\E3")) ("%E0%B9%83")
:set ($convert->("\E4")) ("%E0%B9%84")
:set ($convert->("\E5")) ("%E0%B9%85")
