:local result ""
:local convert ({})
:set ($convert->("\0A")) ("%0A")
:set ($convert->("\20")) ("%20")
:set ($convert->("\A1")) ("%E0%B8%81")
:set ($convert->("\A2")) ("%E0%B8%82")
:set ($convert->("\A3")) ("%E0%B8%83")
:set ($convert->("\A4")) ("%E0%B8%84")
