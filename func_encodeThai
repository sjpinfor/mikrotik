:set ($convert->("\BB")) ("%E0%B8%9B")
:set ($convert->("\BC")) ("%E0%B8%9C")
:set ($convert->("\BD")) ("%E0%B8%9D")
:set ($convert->("\BE")) ("%E0%B8%9E")
:set ($convert->("\BF")) ("%E0%B8%9F")
:set ($convert->("\C0")) ("%E0%B8%A0")
:set ($convert->("\C1")) ("%E0%B8%A1")
:set ($convert->("\C2")) ("%E0%B8%A2")
:set ($convert->("\C3")) ("%E0%B8%A3")
:set ($convert->("\C4")) ("%E0%B8%A4")
:set ($convert->("\C5")) ("%E0%B8%A5")
