:set ($convert->("\ED")) ("%E0%B9%8D")
:set ($convert->("\EE")) ("%E0%B9%8E")
:set ($convert->("\EF")) ("%E0%B9%8F")
:set ($convert->("\F0")) ("%E0%B9%90")
:set ($convert->("\F1")) ("%E0%B9%91")
:set ($convert->("\F2")) ("%E0%B9%92")
:set ($convert->("\F3")) ("%E0%B9%93")
:set ($convert->("\F4")) ("%E0%B9%94")
:set ($convert->("\F5")) ("%E0%B9%95")
:set ($convert->("\F6")) ("%E0%B9%96")
:set ($convert->("\F7")) ("%E0%B9%97")
:set ($convert->("\F8")) ("%E0%B9%98")
:set ($convert->("\F9")) ("%E0%B9%99")
:set ($convert->("\FA")) ("%E0%B9%9A")
:set ($convert->("\FB")) ("%E0%B8%DB")
:set ($convert->("\FC")) ("%E0%B8%DC")
:set ($convert->("\FD")) ("%E0%B8%DD")
:set ($convert->("\FE")) ("%E0%B8%DE")
:set ($convert->("\FF")) ("%E0%B8%DF")
:for i from=0 to=([:len $value] - 1) do={
:local char [:pick $value $i]
:local converted ($convert->"$char")
:local convertedType [:typeof $converted]
:if ($convertedType = "str") do={
:set $char $converted
}
:set result ($result.$char)
}
:return $result


# — — — — ตัวอย่าง script แจ้งเตือนในเมนู /tool netwatch ที่สามารถใส่ข้อความเป็นภาษาไทยได้
#:local message "POE Switch B1 ไม่ทำงาน"
#:local encodeThai [ :parse [ /system script get func_encodeThai source ] ]
#:local encodeMessage [ $encodeThai value=$message ]
#/tool fetch url="https://linenotify.com/notify.php?token=***********แทนที่ด้วย token ของคุณเอง ******************&message=$encodeMessage" mode=http
